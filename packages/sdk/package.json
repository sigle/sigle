{
  "name": "@sigle/sdk",
  "version": "0.0.0",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "codegen": "openapi-typescript http://localhost:3001/_openapi.json --output ./src/__generated__/sigle-api-openapi.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sigle/contracts-source": "workspace:*",
    "zod-validation-error": "3.4.1"
  },
  "devDependencies": {
    "@stacks/connect": "7.9.0",
    "@stacks/network": "7.0.2",
    "@stacks/transactions": "7.0.6",
    "@tsconfig/recommended": "1.0.8",
    "@types/node": "22.15.18",
    "openapi-typescript": "7.8.0",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "vitest": "3.1.3",
    "zod": "3.24.4"
  },
  "peerDependencies": {
    "@stacks/connect": "^7.0.0",
    "@stacks/network": "^7.0.0",
    "@stacks/transactions": "^7.0.0",
    "zod": "^3.23.0"
  }
}
