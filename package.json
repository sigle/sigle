{
  "name": "sigle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "analyze": "ANALYZE=true yarn build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md}'",
    "test:prettier": "prettier --list-different '**/*.{js,ts,tsx,md}'"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@sentry/browser": "5.7.1",
    "@tippy.js/react": "3.1.0",
    "blockstack": "19.3.0",
    "date-fns": "2.6.0",
    "dompurify": "2.0.7",
    "history": "4.10.1",
    "immutable": "4.0.0-rc.12",
    "nanoid": "2.1.6",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-helmet": "5.2.1",
    "react-icons": "3.8.0",
    "react-router-dom": "5.1.2",
    "react-toastify": "5.4.0",
    "react-use": "12.9.1",
    "slate": "0.44.12",
    "slate-html-serializer": "0.7.36",
    "slate-plain-serializer": "0.6.36",
    "slate-react": "0.21.18",
    "slate-soft-break": "0.9.0",
    "styled-components": "4.4.0",
    "typeface-libre-baskerville": "0.0.72",
    "typeface-roboto": "0.0.75"
  },
  "devDependencies": {
    "@craco/craco": "3.6.0",
    "@types/dompurify": "0.0.33",
    "@types/jest": "24.0.20",
    "@types/nanoid": "2.1.0",
    "@types/node": "12.11.7",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/react-helmet": "5.0.13",
    "@types/react-router-dom": "5.1.0",
    "@types/slate": "0.43.8",
    "@types/slate-html-serializer": "0.6.3",
    "@types/slate-plain-serializer": "0.6.1",
    "@types/slate-react": "0.21.0",
    "@types/styled-components": "4.1.20",
    "craco-blockstack": "1.0.2",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "react-scripts": "2.1.8",
    "tailwind.macro": "1.0.0-alpha.10",
    "tailwindcss": "1.1.3",
    "typescript": "3.6.4",
    "webpack-bundle-analyzer": "3.6.0"
  }
}
