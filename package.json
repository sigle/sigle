{
  "name": "sigle",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "test": "TODO",
    "analyze": "BUNDLE_ANALYZE=both next build",
    "analyze:server": "BUNDLE_ANALYZE=server next build",
    "analyze:browser": "BUNDLE_ANALYZE=browser next build",
    "prettier": "prettier --write '**/*.{js,ts,tsx,md,css}'",
    "test:prettier": "prettier --list-different '**/*.{js,ts,tsx,md,css}'"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "@types/slate": "0.47.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@sentry/browser": "5.7.1",
    "@tippy.js/react": "3.1.0",
    "blockstack": "19.3.0",
    "date-fns": "2.6.0",
    "fathom-client": "1.0.0",
    "history": "4.10.1",
    "immutable": "4.0.0-rc.12",
    "nanoid": "2.1.6",
    "next": "9.1.2",
    "next-seo": "2.2.1",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-icons": "3.8.0",
    "react-toastify": "5.4.0",
    "slate": "0.47.8",
    "slate-html-serializer": "0.8.10",
    "slate-plain-serializer": "0.7.10",
    "slate-react": "0.22.9",
    "slate-soft-break": "0.9.0",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.21",
    "@types/nanoid": "2.1.0",
    "@types/node": "12.12.5",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "@types/slate": "0.47.3",
    "@types/slate-html-serializer": "0.6.3",
    "@types/slate-plain-serializer": "0.6.1",
    "@types/slate-react": "0.22.6",
    "@types/styled-components": "4.1.20",
    "@zeit/next-bundle-analyzer": "0.1.2",
    "@zeit/next-css": "1.0.1",
    "babel-plugin-macros": "2.6.1",
    "babel-plugin-styled-components": "1.10.6",
    "dotenv": "8.2.0",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "next-compose-plugins": "2.2.0",
    "prettier": "1.18.2",
    "tailwind.macro": "1.0.0-alpha.10",
    "tailwindcss": "1.1.3",
    "typescript": "3.6.4"
  }
}
