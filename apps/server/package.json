    "@prisma/client": "6.14.0",
    "@sentry/node": "10.5.0",
    "@sigle/sdk": "workspace:*",
    "@stacks/blockchain-api-client": "8.12.0",
    "@stacks/network": "7.0.2",
    "@storacha/client": "1.7.9",
    "@t3-oss/env-core": "0.13.8",
    "@ucanto/principal": "9.0.2",
    "better-auth": "1.3.7",
    "blurhash": "2.0.5",
