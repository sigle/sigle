    "@prisma/client": "6.5.0",
    "@sentry/node": "9.10.0",
    "@sigle/sdk": "workspace:*",
    "@stacks/blockchain-api-client": "8.7.0",
    "@stacks/network": "7.0.2",
    "@t3-oss/env-core": "0.12.0",
    "@ucanto/principal": "9.0.2",
    "@web3-storage/w3up-client": "17.2.0",
    "blurhash": "2.0.5",
    "consola": "3.4.2",
    "h3": "1.15.1",
    "multiformats": "13.3.2",
    "next-auth": "5.0.0-beta.25",
    "nitro-cors": "0.7.1",
    "pg-boss": "10.1.6",
    "posthog-node": "4.10.2",
    "prisma": "6.5.0",
