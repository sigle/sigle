{
  "name": "@sigle/server",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "build": "nitro build",
    "db:generate": "prisma generate",
    "db:migrate:prod": "prisma migrate deploy",
    "dev": "dotenvx run --env-file=.env.development --env-file=.env.development.local -- nitro dev --port 3001",
