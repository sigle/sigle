{
  "name": "@sigle/server",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "build": "nitro build",
    "db:generate": "prisma generate",
    "db:migrate:prod": "prisma migrate deploy",
    "dev": "dotenvx run --env-file=.env.development --env-file=.env.development.local -- nitro dev --port 3001",
    "prepare": "nitro prepare",
    "prisma:migrate:dev": "dotenvx run --env-file=.env.development -- prisma migrate dev",
    "prisma:studio:dev": "dotenvx run --env-file=.env.development -- prisma studio",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ardrive/turbo-sdk": "1.22.0",
    "@dotenvx/dotenvx": "1.43.0",
    "@hirosystems/chainhook-client": "2.3.0",
    "@paralleldrive/cuid2": "2.2.2",
    "@prisma/client": "6.7.0",
    "@sentry/node": "9.17.0",
    "@sigle/sdk": "workspace:*",
    "@sigle/sign-in-with-stacks": "workspace:*",
    "@stacks/blockchain-api-client": "8.10.0",
    "@stacks/network": "7.0.2",
    "@t3-oss/env-core": "0.13.4",
    "@ucanto/principal": "9.0.2",
    "@web3-storage/w3up-client": "17.2.0",
    "better-auth": "1.2.7",
    "blurhash": "2.0.5",
    "consola": "3.4.2",
    "h3": "1.15.3",
    "multiformats": "13.3.3",
    "nitro-cors": "0.7.1",
    "pg-boss": "10.2.0",
    "posthog-node": "4.17.1",
    "prisma": "6.7.0",
    "rate-limiter-flexible": "7.1.0",
    "rou3": "0.5.1",
    "sharp": "0.34.1",
    "zod": "3.24.4",
    "zod-validation-error": "3.4.1"
  },
  "devDependencies": {
    "nitropack": "2.11.11",
    "typescript": "5.8.3"
  }
}
