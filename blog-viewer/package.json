{
  "name": "@sigle/blog-viewer",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "next --port 3001",
    "build": "prisma generate && next build",
    "start": "pnpm run db:migrate && next start",
    "db:migrate": "prisma migrate dev --preview-feature",
    "test": "pnpm run test:only",
    "test:only": "jest --passWithNoTests"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./src/jest.setup.ts"
    ],
    "testEnvironment": "jsdom"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components"
    }
  },
  "dependencies": {
    "@prisma/client": "2.17.0",
    "@sentry/nextjs": "6.17.9",
    "@stacks/auth": "3.1.0",
    "date-fns": "2.25.0",
    "fathom-client": "3.2.0",
    "feed": "4.2.2",
    "immutable": "4.0.0-rc.12",
    "next": "12.1.0",
    "next-seo": "4.28.1",
    "nprogress": "0.2.0",
    "prisma": "2.17.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.2.1",
    "slate": "0.47.9",
    "slate-html-serializer": "0.8.11",
    "slate-plain-serializer": "0.7.11",
    "styled-components": "5.3.1"
  },
  "devDependencies": {
    "@fontsource/roboto": "4.5.1",
    "@tailwindcss/forms": "0.3.4",
    "@tailwindcss/typography": "0.4.1",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@types/jest": "27.4.0",
    "@types/node": "16.10.3",
    "@types/react": "17.0.28",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.15",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "1.13.2",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "fast-xml-parser": "4.0.1",
    "jest": "27.5.1",
    "sass": "1.42.1",
    "tailwindcss": "2.2.15",
    "twin.macro": "2.8.1",
    "typescript": "4.5.5"
  },
  "license": "MIT"
}
